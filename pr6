import java.util.Scanner;
public class faherenheit {

    public static void main(String[] args) {
        System.out.println("220130318035 mahemdavadi riddhi ");
        float c,f;
        Scanner cel= new Scanner(System.in);  
        System.out.println("enter the temperature in celsius:");
        c=cel.nextFloat();
        f=(c*(9/5)+32);
        System.out.println("the temperature in celsius:"+c);
        System.out.println("the temprature in faherenhit:"+f);
        cel.close();
    }
}
